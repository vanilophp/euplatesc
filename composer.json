{
    "name": "vanilo/euplatesc",
    "description": "Euplatesc Payment Gateway Support for Vanilo (Laravel)",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/vanilo/euplatesc"
    },
    "require": {
        "php": "^8.2",
        "konekt/concord": "^1.13",
        "konekt/enum": "^4.2",
        "vanilo/payment": "^4.0",
        "vanilo/contracts": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Vanilo\\Euplatesc\\": "src"
        }
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit" : "^10.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Euplatesc\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
